{
  "name": "@robinblomberg/url",
  "version": "0.7.6",
  "author": "Robin Blomberg",
  "type": "module",
  "main": "lib/index.js",
  "types": "main.d.ts",
  "devDependencies": {
    "@robinblomberg/eslint-config-robinblomberg": "^0.2.20",
    "@robinblomberg/test": "^0.5.7",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "husky": "^4.3.4",
    "lint-staged": "^10.5.3"
  },
  "scripts": {
    "check": "tsc",
    "dev": "nodemon --watch . --exec \"concurrently \\\"npm run --silent test\\\" \\\"npm run --silent check\\\"\"",
    "lint": "eslint **/*.{js,ts}",
    "lint:fix": "eslint **/*.{js,ts} --fix",
    "test": "node ./test/index.test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run check && npm run lint:fix"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "cross-env CI=true eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinBlomberg/url.git"
  },
  "bugs": {
    "url": "https://github.com/RobinBlomberg/url/issues"
  },
  "homepage": "https://github.com/RobinBlomberg/url"
}
